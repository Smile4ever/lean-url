{
  "manifest_version": 2,
  "name": "Lean URL",
  "version": "0.1",

  "description": "Removes evil URL parameters such as Google Analytics' utm parameters",
  "homepage_url": "https://github.com/xmikro/lean-url",

  "icons": {
    "48": "icons/48.png"
  },

  "permissions": [
    "webRequest", "webRequestBlocking"
  ],

  "background": {
    "scripts": ["background.js"]
  }
}